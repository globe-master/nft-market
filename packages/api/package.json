    "@types/jest": "28.1.4",
    "@types/keccak": "3.0.1",
    "@types/node": "16.11.43",
    "@types/secp256k1": "4.0.3",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "babel-jest": "28.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.16.0",
