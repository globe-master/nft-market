    "@fastify/static": "6.4.0",
    "@sinclair/typebox": "0.24.8",
    "@types/bn.js": "^5.1.1",
    "ajv": "8.11.0",
    "bn.js": "^5.2.1",
    "dotenv": "10.0.0",
    "fastify": "4.2.0",
    "fastify-plugin": "3.0.1",
    "keccak": "3.0.2",
    "merkletreejs": "^0.3.9",
    "mongodb": "^4.0.1",
    "pino-pretty": "8.1.0",
