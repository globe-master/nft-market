{
  "name": "wittypixels-ui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --outDir=../api/public",
    "build:production": "vue-tsc --noEmit && vite build --outDir=./dist",
    "build:test": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "start:api": "(cd ../api && pnpm dev) &",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "pnpm build:test && start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "pnpm build:test && pnpm start:api && start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@hennge/vue3-pagination": "1.0.17",
    "@types/lodash.debounce": "^4.0.7",
    "@vueuse/core": "8.5.0",
    "axios": "0.27.2",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.2.2",
    "gsap": "^3.10.4",
    "konva": "^8.4.0",
    "lodash.debounce": "^4.0.8",
    "pinia": "2.0.13",
    "query-string": "7.1.1",
    "sass": "1.45.1",
    "v3-infinite-loading": "^1.0.6",
    "vue": "3.2.33",
    "vue-barcode-reader": "1.0.1",
    "vue-demi": "^0.13.2",
    "vue-konva": "3.0.2",
    "vue-router": "4.0.14",
    "vue-select": "^4.0.0-beta.3",
    "vue3-touch-events": "4.1.0",
    "web3": "1.7.3"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "1.1.3",
    "@types/jsdom": "16.2.14",
    "@types/node": "16.11.34",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.0.0-rc.21",
    "@vue/tsconfig": "0.1.3",
    "autoprefixer": "10.4.7",
    "cypress": "9.7.0",
    "eslint": "8.16.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-vue": "9.0.1",
    "jsdom": "19.0.0",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "unplugin-vue-components": "0.19.5",
    "vite": "2.9.9",
    "vitest": "0.12.9",
    "vue-tsc": "0.34.16"
  }
}
